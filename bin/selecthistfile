#!/bin/bash

SESSIONPATH=/usr/lib/ws-session

source $SESSIONPATH/lib/rc.sh

source $SESSIONPATH/lib/wm.sh

if [[ $WM = "NULL" ]] ; then
  echo "$HOME/.$S_SHELL_HISTORY"
  exit 0
fi

DIR="$S_TEMPFOLDER/$SELTAG"
FILE="$DIR/$S_SHELL_HISTORY"
[[ -f $FILE ]] || mkdir -p "$DIR" && touch "$FILE"

echo $FILE

# vim: ft=sh ts=2 et sw=2:

