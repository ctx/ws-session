#!/bin/bash
cmd=${0##*/}

S_LIB_FOLDER=${S_LIB_FOLDER:-/usr/lib/ws-session}
if source $S_LIB_FOLDER/core/source.sh 2>/dev/null; then
  s_source_lib wm

  if [[ -n $S_WM ]] ; then
    s_source app/${cmd}.sh
    s_${cmd}_start "$@"
    disown -a
    exit 0
  fi
fi

/usr/bin/$cmd $@

# vim: ft=sh ts=2 et sw=2:
